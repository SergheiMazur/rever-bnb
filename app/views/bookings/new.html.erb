<h1>Bookings#new</h1>

<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Create a new Rever game booking!</h1>
      <%= simple_form_for @booking do |f| %>
        <%= f.input :status %>
        <%= f.input :location %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>

